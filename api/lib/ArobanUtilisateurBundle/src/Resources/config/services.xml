<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="aroban_utilisateur.security_controller" class="Aroban\Bundle\UtilisateurBundle\Controller\SecurityController" public="true">
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
            <tag name="container.service_subscriber"/>
            <argument type="service" id="ApiPlatform\Core\Api\IriConverterInterface" key="$iriConverter"/>
        </service>
    </services>
</container>
